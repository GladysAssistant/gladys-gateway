{
  "name": "gladys-gateway-server",
  "version": "1.0.0",
  "description": "An End-to-End Encrypted Gateway to access Gladys from the internet",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/bootstrap.test.js ./test/**/*.test.js --exit",
    "coverage": "nyc --reporter=lcov npm test",
    "eslint": "eslint core test index.js",
    "eslint-fix": "eslint --fix core test index.js",
    "prettier": "prettier --write core test index.js",
    "prettier-check": "prettier --check core test index.js",
    "start": "nodemon index.js",
    "apidoc": "apidoc -i core/ -o apidoc/",
    "setup-db-dev": "db-migrate up -e dev",
    "setup-db-test": "db-migrate up -e test",
    "start-email-preview": "node ./core/common/dev-email-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gladysassistant/gladys-gateway.git"
  },
  "keywords": [
    "gladys",
    "gateway",
    "end-to-end",
    "encryption",
    "nodejs",
    "home",
    "automation"
  ],
  "author": "Pierre-Gilles Leymarie",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gladysassistant/gladys-gateway/issues"
  },
  "apidoc": {
    "title": "Gladys Gateway",
    "name": "Gladys Gateway"
  },
  "homepage": "https://github.com/gladysassistant/gladys-gateway#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^10.0.0",
    "nock": "^13.2.9",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "pify": "^5.0.0",
    "prettier": "^2.3.2",
    "should": "^13.2.3",
    "socket.io-client": "^4.5.3",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "@googleapis/homegraph": "^4.0.0",
    "@sentry/node": "^5.22.3",
    "@socket.io/redis-adapter": "^7.2.0",
    "async-retry": "^1.3.3",
    "aws-sdk": "^2.1129.0",
    "axios": "^1.1.3",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "bottleneck": "^2.19.5",
    "clone": "^2.1.2",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^6.2.0",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^3.2.1",
    "geoip-lite": "^1.4.0",
    "get-value": "^3.0.1",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "massive": "^6.9.0",
    "node-telegram-bot-api": "^0.53.0",
    "nodemailer": "^6.7.5",
    "object-sizeof": "^1.4.0",
    "omit-deep": "^0.3.0",
    "querystring": "^0.2.0",
    "rate-limit-redis": "^1.5.0",
    "rate-limiter-flexible": "^2.3.7",
    "redis": "^2.8.0",
    "secure-remote-password": "^0.3.1",
    "socket.io": "^4.5.3",
    "speakeasy": "^2.0.0",
    "stripe": "^8.137.0",
    "tracer": "^0.9.1",
    "uuid": "^3.3.2"
